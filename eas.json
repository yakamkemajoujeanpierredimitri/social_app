{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
 "preview": {
      "android": {
        "buildType": "apk"
      },
        "env": {
        "APP_ENV": "production",
        "GOOGLE_SERVICES_JSON": "./google-services.json"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
